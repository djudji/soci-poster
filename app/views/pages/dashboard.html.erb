<div class="row">

  <div class="col-md-2 text-center">
    <%= image_tag 'soci_poster.svg', class: 'logo' %>
    <hr>

    <div class="time-zone">
      <p class="text-center">
        <b><%= Time.zone.name %> Timezone </b>
        <%= link_to 'change', edit_user_registration_path %>
      </p>
    </div>

    <div class="social">
      <%= render partial: 'provider', locals: { provider: 'twitter', provider_class: 'info'} %>
      <%= render partial: 'provider', locals: { provider: 'facebook', provider_class: 'primary'} %>
    </div>
  </div>

  <div class="col-md-5">
    <h3 class="text-center">Scheduled</h3>
    <hr>
    <%= render @scheduled %>
    <%= will_paginate @scheduled, :param_name => 'scheduled_page' %>
  </div>

  <div class="col-md-5">
    <h3 class="text-center">History</h3>
    <hr>
    <%= render @history %>
    <%= will_paginate @history, :param_name => 'scheduled_page' %>
  </div>

</div>
